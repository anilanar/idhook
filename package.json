{
  "name": "idhook",
  "version": "1.0.1",
  "description": "Generate unique ids to use in your HTML with a very intuitive API.",
  "keywords": [
    "react",
    "unique",
    "id",
    "form",
    "formik",
    "typescript"
  ],
  "author": "anilanar",
  "license": "MIT",
  "repository": "anilanar/idhook",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test",
    "test:watch": "react-scripts test",
    "build": "rollup -c && tsc -d --emitDeclarationOnly --noEmit false --declarationDir dist",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^16.9.0 || ^17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/runtime": "^7.3.1",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@rollup/plugin-url": "^5.0.1",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^23.3.13",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "cross-env": "^5.2.0",
    "gh-pages": "^2.0.1",
    "nanoid": "^3.1.25",
    "react": "^17.0.2",
    "react-scripts": "^4.0.3",
    "react-test-renderer": "^17.0.2",
    "rollup": "^2.26.9",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "tslib": "^2.0.1",
    "typescript": "^3.2.4"
  }
}
